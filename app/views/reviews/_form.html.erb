<%= form_with model: [ @artist, @artist.reviews.build ], html: {multipart: true} do |form| %>
  <div>
    <%= form.label :rating %>
    <%= form.select :rating, (1..5).to_a %>
  </div>
  <div>
    <%= form.label :comment %>
    <%= form.text_area :comment %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
